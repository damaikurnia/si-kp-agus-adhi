	<queryString><![CDATA[SELECT
	a.nama_pemilik, a.nip_lama, e.nip_baru, d.tempat_lahir, DATE_FORMAT(d.tanggal_lahir, '%d-%m-%Y') AS tanggal_lahir,
	a.pangkat_baru, a.golongan_baru,ROUND(DATEDIFF(CURRENT_DATE,d.tanggal_surat)/365,0) AS tahun,
	ROUND(MOD(DATEDIFF(CURRENT_DATE,d.tanggal_surat)/365,12)) AS Bulan,e.gajipokok_baru,d.Profesi, f.id_SuratKarpeg,
	DATE_FORMAT(g.tanggal_nikah, '%d-%m-%Y') AS tanggal_nikah,e.Sekolah, h.Alamat, DATE_FORMAT(a.tmt_baru, '%d-%m-%Y') AS tmt,
	DATE_FORMAT(CURRENT_DATE, '%d-%m-%Y') AS tanggalsekarang,
	YEAR(d.tanggal_lahir)+60 AS Tahun_TMT,
	MONTH(d.tanggal_lahir)+1 AS bulan_TMT
FROM
	sk_pangkatterakhir a, pns b, sk_cpns d, sptkg_terakhir e, sk_karpeg f, s_nikah g, kk h
WHERE
	b.nip_baru LIKE $P{NIP} AND
	b.id_SuratPangkatTerakhir=a.id_SuratPangkatTerakhir AND
	b.id_SuratCPNS=d.id_SuratCPNS AND
	b.id_SuratSPTKGTerakhir	= e.id_SuratSPTKGTerakhir AND
	b.id_SuratKarpeg = f.id_SuratKarpeg AND
	b.id_SuratNikah	= g.id_SuratNikah AND
	b.id_Suratkk = h.id_Suratkk;]]></queryString>

